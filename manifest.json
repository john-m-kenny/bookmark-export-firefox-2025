{
  "manifest_version": 3,
  "name": "twitter/x bookmark exporter",
  "version": "1.0",
  "description": "export your twitter/x bookmarks to a json file.",
  "permissions": [
    "storage",
    "webRequest",
    "downloads"
  ],
  "host_permissions": [
    "*://x.com/*",
    "*://twitter.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}